pipeline{
    agent any
    
    environment{
         PATH = "/opt/apache-maven-3.8.5/bin:$PATH"
         }
         
    stages{
        stage("git checkout"){
            steps{
               git credentialsId: 'github', url: 'https://github.com/avi183/my-app'
            }
        }

         stage(" maven build "){
             steps{
                sh "mvn clean package"
             }       
         }
    }
}
